{
    "manifest_version": 2,
    "name": "Bookmarkie",
    "author": "Mensur Owary",
    "version": "0.0.1",
    "description": "Bookmarks and shit",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "background": {
        "scripts": ["scripts/background.js"],
        "persistent": false
    },
    "browser_action": {},
    "permissions": [
        "storage",
        "alarms",
        "bookmarks",
        "tabs",
        "http://*/",
        "https://*/"
    ]
}
