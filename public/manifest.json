{
  "manifest_version": 2,
  "name": "Bookmarked React",
  "author": "Mensur Owary",
  "version": "0.0.1",
  "description": "Google Chrome extension that allows you to have a todo list of the websites you would like to visit",
  "chrome_url_overrides": {
      "newtab": "index.html"
  },
  "icons": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
  },
  "background": {
      "scripts": [
          "scripts/background/background.js",
          "scripts/background/metaparser.js",
          "scripts/background/uuid.js"
      ],
      "persistent": false
  },
  "browser_action": {
      "default_icons": {
          "16": "icons/16.png",
          "48": "icons/48.png",
          "128": "icons/128.png"
      },
      "default_title": "Bookmark!"
  },
  "permissions": [
      "storage",
      "alarms",
      "bookmarks",
      "tabs",
      "http://*/",
      "https://*/"
  ]
}
